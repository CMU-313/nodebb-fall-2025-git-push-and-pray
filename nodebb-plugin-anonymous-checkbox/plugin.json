{
    "id": "nodebb-plugin-anonymous-checkbox",
    "name": "Anonymous Checkbox",
    "library": "library.js",
    "hooks": [
      { "hook": "static:app.load", "method": "init", "priority": 10 },
      { "hook": "filter:post.create", "method": "filterPostData", "priority": 5 },
      { "hook": "action:post.create", "method": "afterPostCreate", "priority": 10 },
      { "hook": "filter:post.get", "method": "maskPost", "priority": 10 },
      { "hook": "filter:posts.get", "method": "maskPosts", "priority": 10 }
    ],
    "scripts": ["static/lib/composer-anonymous.js"]
  }